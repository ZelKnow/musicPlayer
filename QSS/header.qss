QPushButton{
    border: none;
    font: bold;
    font-size: 18px;
}

QPushButton#loginButton {
    font-size: 11px;
    color: #ADAFB2;
}

QPushButton#loginButton:hover {
    color: #DCDDE4;
}

QPushButton#closeButton, QPushButton#minButton, QPushButton#maxButton {
    color: #828487;
}

QPushButton#closeButton::hover, QPushButton#minButton::hover, QPushButton#maxButton::hover {
    color: #DCDDE4;
}

QPushButton#prevButton, QPushButton#nextButton {
    color: #828487;
}

QPushButton#prevButton::hover, QPushButton#nextButton::hover {
    color: #DCDDE4;
}

QLabel{
    font: 75 14pt "Agency FB";
    color: #A3A5A8;
}

QFrame#line1{
    color: #161619;
}